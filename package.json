{
  "name": "@m0t0r/ngx-slider",
  "version": "1.0.0",
  "private": true,
  "description": "A customizable slider component for Angular >= 9 based on https://github.com/angular-slider/ng5-slider",
  "keywords": [
    "slider",
    "ui",
    "component",
    "angular",
    "ng",
    "ngx"
  ],
  "author": "Vitaly Tkachenko, Piotr Dziwinski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/m0t0r/ngx-slider.git"
  },
  "bugs": {
    "url": "https://github.com/m0t0r/ngx-slider/issues"
  },
  "homepage": "https://github.com/m0t0r/ngx-slider#readme",
  "scripts": {
    "ng": "ng",
    "prepare": "npm run prepare:lib && npm run prepare:demo-app",
    "prepare:lib": "ng build ngx-slider --prod",
    "prepare:demo-app": "node scripts/generate-demo-app-snippets.js && node scripts/generate-demo-app-docs.js",
    "start": "npm run prepare && ng serve",
    "build": "npm run build:demo-app",
    "build:lib": "npm run prepare && rimraf dist/ngx-slider dist/ngx-slider.tgz && ng-packagr -p projects/ngx-slider/package.json && scss-bundle -e projects/ngx-slider/src/lib/main.scss -d dist/ngx-slider/scss/ngx-slider.scss",
    "build:demo-app": "npm run prepare && rimraf dist/demo-app && ng build --prod --base-href /ngx-slider/",
    "publish-gh": "scripts/publish-gh-pages.sh",
    "test": "npm run prepare && ng test ngx-slider",
    "test:once": "npm run prepare && ng test ngx-slider --watch=false",
    "lint": "npm run prepare && ng lint",
    "e2e": "npm run prepare && ng e2e",
    "ci": "npm run test:once && npm run e2e && npm run lint && npm run prepare:lib && npm run build:demo-app"
  },
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/localize": "^9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@ng-bootstrap/ng-bootstrap": "5.2.2",
    "detect-passive-events": "1.0.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-devkit/build-ng-packagr": "^0.901.7",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "escape-html": "1.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "mkdirp": "0.5.1",
    "ng-packagr": "=9.0.0",
    "prismjs": "1.14.0",
    "protractor": "~5.4.3",
    "protractor-console-plugin": "0.1.1",
    "rimraf": "2.6.2",
    "scss-bundle": "2.4.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.37.1",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "typedoc": "0.16.9",
    "typescript": "^3.7.5"
  }
}
